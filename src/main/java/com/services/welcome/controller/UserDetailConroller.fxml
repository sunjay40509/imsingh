<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.services.welcome.controller.UserDetailConroller">
  <!-- TODO Add Nodes -->
  <children>
    <VBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <VBox id="VBox" alignment="CENTER" spacing="10.0">
          <children>
            <HBox alignment="CENTER_RIGHT" prefHeight="-1.0" prefWidth="-1.0">
              <children>
                <Button mnemonicParsing="false" onAction="#logoutButton_OnAction" text="Logout" />
              </children>
            </HBox>
            <VBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0">
              <children>
                <GridPane hgap="5.0" vgap="3.0">
                  <children>
                    <Label text="First Name :" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                    <Label text="Last Name :" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                    <Label text="User Name :" GridPane.columnIndex="0" GridPane.rowIndex="2" />
                    <Label text="Emai Id :" GridPane.columnIndex="0" GridPane.rowIndex="3" />
                    <Label fx:id="firstNameLabel" minWidth="30.0" text="" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                    <Label fx:id="lastNameLabel" minWidth="30.0" text="" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    <Label fx:id="userNameLabel" minWidth="30.0" text="" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                    <Label fx:id="emailIdLabel" minWidth="30.0" text="" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  </children>
                  <columnConstraints>
                    <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="-1.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  </columnConstraints>
                  <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  </rowConstraints>
                </GridPane>
              </children>
            </VBox>
          </children>
        </VBox>
      </children>
    </VBox>
  </children>
</AnchorPane>
